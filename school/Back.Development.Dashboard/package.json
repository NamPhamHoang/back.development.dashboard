{
  "name": "back.development.dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "codegen": "env NODE_ENV=development apollo client:codegen --config=src/graphql/apollo.config.js --target=typescript --tagName=gql --outputFlat src/graphql/generated",
    "dev": "env NODE_ENV=development nodemon",
    "start": "env NODE_ENV=production node -r ./dotenv.config -r module-alias/register dist/index.js",
    "start:bg": "env NODE_ENV=production forever start dist/index.js",
    "stop:bg": "forever stopall",
    "log": "forever logs 0",
    "db:backup": "docker-compose -f docker-compose.yml exec -u postgres -T postgres pg_dump > ./backup/$(date +\"%m-%d-%y-%H-%m-%S\").dump",
    "db:restore": "docker-compose -f docker-compose.yml exec -u postgres -T postgres psql <",
    "db:staging:restore": "docker-compose -f docker-compose.staging.yml exec -u postgres -T eric_staging_db psql <",
    "db:prod:backup": "yarn dc:prod exec -u postgres -T postgres pg_dump > ./backup/$(date +\"%m-%d-%y-%H-%m-%S\").dump",
    "db:prod:restore": "yarn dc:prod exec -u postgres -T postgres psql <",
    "dc:dev": "docker-compose -f docker-compose.yml",
    "dc:prod": "docker-compose -f docker-compose.prod.yml",
    "build": "env NODE_ENV=production tsc -p tsconfig.prod.json",
    "deploy:build": "npm run build && docker-compose -f docker-compose.prod.yml build app"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.3",
    "@sentry/node": "5.15.5",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/request": "^2.48.4",
    "apollo": "^2.30.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-ws": "^1.0.19",
    "axios": "^0.21.0",
    "babel-plugin-import": "^1.13.0",
    "bcrypt": "^4.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "devergroup-error": "^1.0.0",
    "dotenv-safe": "^6.0.0",
    "error-stack-parser": "^2.0.4",
    "express": "^4.17.1",
    "forever": "^1.0.0",
    "glob": "^7.1.6",
    "graphql-tag": "^2.10.1",
    "http-proxy-middleware": "^1.0.3",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^26.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "minimist": "1.2.5",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "next": "^9.5.3",
    "onesignal-node": "^3.1.0",
    "request": "^2.88.2",
    "sockjs-client": "^1.5.0",
    "ts-jest": "^26.3.0",
    "tsconfig-paths": "^3.9.0",
    "winston": "^3.0.0",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/express": "^4.17.6",
    "@types/fb": "^0.0.28",
    "@types/jest": "^26.0.7",
    "@types/lodash": "^4.14.155",
    "@types/sockjs-client": "^1.1.1",
    "@types/yup": "^0.29.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "concurrently": "^5.2.0",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "precise-commits": "^1.0.2",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "_moduleAliases": {
    "~@": "./dist"
  }
}
